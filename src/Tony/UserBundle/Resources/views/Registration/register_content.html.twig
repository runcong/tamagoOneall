{% trans_default_domain 'FOSUserBundle' %}



<div class="row col-md-8 col-md-offset-2">
        <div class="panel panel-info text-center">
            <div class="panel-heading">
                <h3 class="panel-title">Become a Member</h3>
            </div>
            <div class="panel-body">
                <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register contact">
                    <div class="form-group contact form">
                        {{ form_widget(form) }}
                    </div>

                <div class="form-group">
                <input type="submit" class="btn btn-info" value="{{ 'registration.submit'|trans }}" />

                </div>

                </form>

            </div>
        </div>
</div>

