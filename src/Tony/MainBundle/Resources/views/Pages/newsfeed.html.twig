<div class="col-sm-10  col-sm-offset-1">
    {% for feed in pagerfanta %}
        <h3><a href="{{ feed.link }}" title="{{ feed.title }}">{{ feed.title }}</a></h3>
        <small><em>Posted on {{ feed.date }}</em></small>
        <p>{{ feed.desc|raw }}</p>
    {% endfor %}
    {{ pagerfanta(pagerfanta, 'twitter_bootstrap3') }}
</div>