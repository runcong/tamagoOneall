<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Co Grade{% endblock %}</title>
    <meta name=“keywords” content = “job, student, grade, company”>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" href="{{ asset('bundles/tonymain/css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/tonymain/css/font-awesome.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/tonymain/css/custom.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/tonymain/css/star-rating.min.css') }}" media="all" rel="stylesheet" type="text/css">

</head>

<body>
{{ oneall_initialize() }}

<div class="container-fluid">
    <header class="row">
        <div class="col-sm-4"> <a href=""><img src="{{ asset('bundles/tonymain/images/logo.jpg') }}" alt="company logo" id="co_logo"></a> </div>
        <div class="col-sm-8">

            <div class="navbar-collapse collapse navbar-right" >
                <ul class="nav navbar-nav">
                    <li><a href="{{ path('fos_user_profile_edit') }}">ADMIN</a></li>
                    <li><a href="{{ path('fos_user_profile_show') }}">ACCOUNT</a></li>
                    <li><a href="{{ path('fos_user_security_logout') }}">LOGOUT</a></li>
                </ul>
            </div><!--/.nav-collapse -->


            <!-- inside a twig template -->
            {{ oneall_login_button({'login_container_id': 'some_tag_id'}) }}


        </div>
    </header>
    <div class="navbar navbar-default row">
        <ul class="nav navbar-nav">
            <li class="{% if app.request.attributes.get('_route') == 'tony_main_home' %}active{% endif %}"><a href="{{ path('tony_main_home') }}">HOME</a></li>
            <li class="{% if app.request.attributes.get('_route') == 'tony_main_listing' %}active{% endif %}"><a href="{{ path('tony_main_listing') }}">COMPANY & REVIEWS</a></li>
            <li class="{% if app.request.attributes.get('_route') == 'tony_main_jobs' %}active{% endif %}"><a href={{ path('tony_main_jobs') }}>JOBS</a></li>
            <li class="{% if app.request.attributes.get('_route') == 'tony_main_contact' %}active{% endif %}"><a href="{{ path('tony_main_contact') }}">CONTACT</a></li>
            <li class="{% if app.request.attributes.get('_route') == 'tony_main_signup' %}active{% endif %}"><a href="{{ path('fos_user_registration_register') }}">SIGN UP</a></li>
            <li class="{% if app.request.attributes.get('_route') == 'tony_main_login' %}active{% endif %}"><a href="{{ path('fos_user_security_login') }}">LOGIN</a></li>
        </ul>
    </div>
    <div class="content row">

{% block content %} {% endblock %}


    </div><!--Content end-->


</div><!--container-->


<footer>
    <p>&copy; 2015 Dev-Team@UMB</p>


</footer>


<script src="{{ asset('bundles/tonymain/js/jquery-1.11.1.min.js') }}"></script>
<script src="{{ asset('bundles/tonymain/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('bundles/tonymain/js/star-rating.min.js') }}" type="text/javascript"></script>



</body>
</html>